{
  "manifest_version": 2,
  "name": "Stack Exchange Zen",
  "description": "Removes recommended section on Stack Exchange sites",
  "version": "1.0.0",
  "permissions": ["tabs"],
  "icons": {
    "400": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.stackexchange.com/*", "*://*.stackoverflow.com/*"],
      "css": ["inject.css"]
    }
  ]
}
