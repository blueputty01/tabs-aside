{
  "manifest_version": 3,
  "name": "Tabs Aside",
  "short_name": "Tabs Aside",
  "description": "Remembers tabs for later so you don't have to.",
  "version": "4.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "app.html?popup=true"
  },
  "options_page": "options.html",
  "permissions": [
    "favicon",
    "storage",
    "unlimitedStorage",
    "tabs"
  ],
  "offline_enabled": true
}